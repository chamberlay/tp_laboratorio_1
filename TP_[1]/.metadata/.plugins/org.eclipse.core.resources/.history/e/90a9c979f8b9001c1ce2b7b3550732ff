#include "TP_[1]Calculos.h"

float calcularPrecioConDebito(float precio)
{
    float resultado;

    resultado = precio - (precio * 0.10);

    return resultado;
}

float calcularPrecioConCredito(float precio)
{
    float resultado;

    resultado = precio + (precio * 0.25);

    return resultado;
}
